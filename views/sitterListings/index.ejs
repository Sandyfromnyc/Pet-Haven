<%- include('../partials/header') %>


<table id="'Sitters"> 
    <thead>
     <tr>
        <th>Sitters</th>
    </tr>
</thead>
<tbody>

    
    <% sitterListings.forEach(function(s) {  %>
    <tr>
      <td><%= s.userName %></td>
       <td> <%= s.location %></td>
       <td><img src="<%= s.userAvatar %>"></td>
       <td><a href="/sitterListings/<%= s.id %>">Details</a></td>
    </tr>
    <% }); %>
        
    </tbody>
</table>

<%- include('../partials/footer') %>
